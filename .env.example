# Google Calendar API credentials
GOOGLE_CALENDAR_ID=primary

# Pushover credentials
PUSHOVER_USER_KEY=your_user_key_here
PUSHOVER_API_TOKEN=your_api_token_here

# Optional: Custom time for daily summary (24-hour format)
SUMMARY_TIME=08:00

# Optional: Custom time for tomorrow's summary (24-hour format)
TOMORROW_SUMMARY_TIME=22:00

# Optional: Timezone (defaults to America/New_York for EST)
TIMEZONE=America/New_York